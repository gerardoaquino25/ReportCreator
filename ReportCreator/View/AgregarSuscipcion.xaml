<UserControl x:Class="ReportCreator.View.AgregarSuscipcion"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:controls="clr-namespace:ReportCreator.View.UtilityElement"
             mc:Ignorable="d" 
             Height="Auto" Width="Auto">
    <Grid>
        <Border Name="Prueba" Style="{StaticResource CentroStyle}">
            <StackPanel>
                <controls:ExternoExistenteUE/>
                <controls:ExternosUE/>
                <controls:InternosUE x:Name="Internos"/>
                <controls:NombreExternoUE/>
                <controls:ObservacionExternoUE/>
                <controls:ObservacionUE/>
            </StackPanel>
        </Border>
    </Grid>
</UserControl>
